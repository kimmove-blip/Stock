def calculate_signal_weight(signals):
    from IndicatorWeights
    
    total_weight = 0
    weights_dict = vars(IndicatorWeights)
    
    for signal in signals:
        weight = weights_dict.get(signal, 0)
        if weight:
            total_weight += weight
    
    return total_weight


