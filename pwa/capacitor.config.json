{
  "appId": "com.kimsai.stock",
  "appName": "Kim's AI 주식분석",
  "webDir": "dist",
  "server": {
    "url": "https://stock.kims-ai.com",
    "cleartext": false
  },
  "ios": {
    "contentInset": "automatic",
    "allowsLinkPreview": true,
    "scrollEnabled": true,
